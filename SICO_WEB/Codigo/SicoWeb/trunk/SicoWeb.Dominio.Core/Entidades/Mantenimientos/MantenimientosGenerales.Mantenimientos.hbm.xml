<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping default-cascade="save-update" namespace="SicoWeb.Dominio.Core.Entidades.Mantenimientos" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Mantenimientos" table="Mantenimientos" discriminator-value="AMantenimientos">
    <id name="Id" type="Int32">
      <column name="Id" />
      <generator class="increment" />
    </id>
    <discriminator>
      <column name="Class" not-null="true" />
    </discriminator>
    <property name="Descripcion" type="String">
      <column name="Descripcion" />
    </property>
    <property name="Habilitado" type="Boolean">
      <column name="Habilitado" />
    </property>
    <property name="Usu" type="Int32">
      <column name="Usu" />
    </property>
    <property name="Fmodif" type="DateTime">
      <column name="Fmodif" />
    </property>
  </class>
</hibernate-mapping>